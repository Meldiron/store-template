{
    "projectId": "store",
    "projectName": "store",
    "settings": {
        "services": {
            "account": true,
            "avatars": true,
            "databases": true,
            "locale": true,
            "health": true,
            "storage": true,
            "teams": true,
            "users": true,
            "functions": true,
            "graphql": true,
            "messaging": true
        },
        "auth": {
            "methods": {
                "jwt": true,
                "phone": true,
                "invites": true,
                "anonymous": true,
                "email-otp": true,
                "magic-url": true,
                "email-password": true
            },
            "security": {
                "duration": 31536000,
                "limit": 0,
                "sessionsLimit": 10,
                "passwordHistory": 0,
                "passwordDictionary": false,
                "personalDataCheck": false
            }
        }
    },
    "functions": [
        {
            "$id": "stripe-proxy",
            "execute": [
                "any"
            ],
            "name": "stripe-proxy",
            "enabled": true,
            "logging": true,
            "runtime": "node-22",
            "scopes": [
                "databases.read",
                "databases.write",
                "collections.write",
                "attributes.write",
                "documents.read",
                "documents.write"
            ],
            "events": [],
            "schedule": "",
            "timeout": 15,
            "entrypoint": "src/main.js",
            "commands": "npm install",
            "path": "functions/stripe-proxy"
        }
    ]
}